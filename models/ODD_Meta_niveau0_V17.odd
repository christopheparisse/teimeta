				<elementSpec ident="persName" module="namesdates">
					<desc xml:lang="fr">Nom du locuteur</desc>
					<desc xml:lang="en">Speaker's name</desc>
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="socecStatus" module="namesdates">
					<desc xml:lang="fr">Indice socio-économique</desc>
					<desc xml:lang="en">Social-economic Status</desc>
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="education" module="namesdates">
					<desc xml:lang="fr">Niveau de scolarité</desc>
					<desc xml:lang="en">Education level</desc>
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="altGrp" module="core">
					<content>
						<elementRef key="alt" minOccurs="1" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="alt" module="core">
					<desc xml:lang="fr">Pseudonyme (dans la transcritpion)</desc>
					<desc xml:lang="en">Pseudonym (in transcript)</desc>
					<attList>
						<attDef ident="type">
							<desc>Type</desc>
							<datatype>
								<dataRef name="string"/>
							</datatype>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="note" module="core">
					<desc xml:lang="fr">Information supplémentaire</desc>
					<desc xml:lang="en">Additional Information</desc>
					<content>
						<textNode/>
					</content>
					<attList>
						<attDef ident="type">
							<desc xml:lang="fr">Type</desc>
							<desc xml:lang="en">Type</desc>
							<datatype>
								<dataRef name="string"/>
							</datatype>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="age" module="namesdates" rend="age.adult">
					<desc xml:lang="fr">Tranche d'âge</desc>
					<desc xml:lang="en">Age Interval</desc>
					<content>
						<valList>
							<valItem ident="age.adult">
								<desc xml:lang="fr">de 21 à 60 ans</desc>
								<desc xml:lang="en">from 21 to 60</desc>
							</valItem>
							<valItem ident="age.infant">
								<desc xml:lang="fr">moins de 1 an</desc>
								<desc xml:lang="en">less than 1</desc>
							</valItem>
							<valItem ident="age.youngchild">
								<desc xml:lang="fr">de 1 à 2 ans</desc>
								<desc xml:lang="en">from 1 to 2</desc>
							</valItem>
							<valItem ident="age.middlechild">
								<desc xml:lang="fr">de 3 à 5 ans</desc>
								<desc xml:lang="en">from 3 to 5</desc>
							</valItem>
							<valItem ident="age.oldchild">
								<desc xml:lang="fr">de 6 à 15 ans</desc>
								<desc xml:lang="en">from 6 to 15</desc>
							</valItem>
							<valItem ident="age.youth">
								<desc xml:lang="fr">de 16 à 20 ans</desc>
								<desc xml:lang="en">from 16 to 20</desc>
							</valItem>
							<valItem ident="age.senior">
								<desc xml:lang="fr">plus de 61 ans</desc>
								<desc xml:lang="en">up to 61</desc>
							</valItem>
							<valItem ident="age.unknown">
								<desc xml:lang="fr">âge inconnu</desc>
								<desc xml:lang="en">unknown age</desc>
							</valItem>
						</valList>
					</content>
					<attList>
						<attDef ident="value" usage="req" mode="change">
							<datatype>
								<dataRef name="number"/>
							</datatype>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="occupation" module="namesdates" rend="occupation.professional_activity">
					<desc xml:lang="fr">Situation</desc>
					<desc xml:lang="en">Occupation</desc>
					<attList>
						<attDef ident="ana" usage="req" mode="change">
							<valList>
								
								<valItem ident="occupation.before_school">
									<desc xml:lang="fr">avant la scolarisation</desc>
									<desc xml:lang="en">before school</desc>
								</valItem>
								<valItem ident="occupation.at_school">
									<desc xml:lang="fr">en scolarité</desc>
									<desc xml:lang="en">scholar</desc>
								</valItem>
								<valItem ident="occupation.professional_activity">
									<desc xml:lang="fr">en activite</desc>
									<desc xml:lang="en">in professional situation</desc>
								</valItem>
								<valItem ident="occupation.retirement">
									<desc xml:lang="fr">retraite</desc>
									<desc xml:lang="en">retirement</desc>
								</valItem>
								<valItem ident="occupation.without_job">
									<desc xml:lang="fr">sans emploi (chômage, H/F au foyer) </desc>
									<desc xml:lang="en">without job (unemployed, at home) </desc>
								</valItem>
								<valItem ident="occupation.unknown">
									<desc xml:lang="fr">inconnue</desc>
									<desc xml:lang="en">unknown</desc>
								</valItem>
							</valList>
						</attDef>
					</attList>
				</elementSpec>

				<elementSpec ident="langKnowledge" module="namesdates">
					<content>
						<elementRef key="langKnown" minOccurs="1" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="langKnown" module="namesdates">
					<desc xml:lang="fr">Langue des locuteurs</desc>
					<desc xml:lang="en">Speaker's language</desc>
					<attList>
						<!-- slangue par défaut français, pour le moment inutile car liste des codes iso non reportée-->
						<attDef ident="tag" usage="req"
							rendition="code iso des langues, cf langUsage pour l'enregistrement"
							mode="change" rend="français">
							<desc xml:lang="fr">Code Iso</desc>
							<desc xml:lang="en">Iso code</desc>
							<datatype>
								<dataRef name="string"/>
							</datatype>
						</attDef>
						<attDef ident="level" usage="req" mode="change" rend="language.first">
							<valList>
								<valItem ident="language.first">
									<desc xml:lang="fr">maternelle</desc>
									<desc xml:lang="en">first language</desc>
								</valItem>
								<valItem ident="language.second">
									<desc xml:lang="fr">seconde</desc>
									<desc xml:lang="en">second</desc>
								</valItem>
								<valItem ident="language.unknown">
									<desc xml:lang="fr">inconnue</desc>
									<desc xml:lang="en">unknown</desc>
								</valItem>
							</valList>
						</attDef>
					</attList>
				</elementSpec>

				<!-- settingDesc : recording et medias-->
				<elementSpec ident="settingDesc" module="corpus" mode="change" n="1">
					<desc xml:lang="fr">Session enregistrée : lieu, langue,date</desc>
					<desc xml:lang="en">Seeting: place, language,date</desc>

					<content>
						<sequence minOccurs="1" maxOccurs="1">
							<elementRef key="listPlace"/>
							<elementRef key="setting"/>
							<!-- langUsage : langue de l'enregistrement, plusieurs possibles avec % d'utilisation-->
							<elementRef key="langUsage"/>
						</sequence>
					</content>
				</elementSpec>

				<elementSpec ident="listPlace" module="namesdates">
					<content>
						<elementRef key="place" minOccurs="1" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="place" module="namesdates">
					<desc xml:lang="fr">Lieu</desc>
					<desc xml:lang="en">Place</desc>
					<content>
						<sequence minOccurs="1" maxOccurs="1">
							<elementRef key="placeName"/>
							<elementRef key="country"/>
						</sequence>
						<elementRef key="settlement" minOccurs="0" maxOccurs="unbounded"/>
						<elementRef key="district" minOccurs="0" maxOccurs="unbounded"/>
						<!-- TODO : plusieurs desc ??? desc:  piece, exemple changement de lieu avec micro cravate -->
						<elementRef key="desc" minOccurs="0" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="placeName" module="namesdates">
					<desc xml:lang="fr">Lieu</desc>
					<desc xml:lang="en">Place</desc>
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="country" module="namesdates">
					<desc xml:lang="fr">Pays</desc>
					<desc xml:lang="en">Country</desc>
					<attList>
						<!-- attribut ref= norme iso ISO 3366-1 => Indiquer le pays de recueil , donnée minimale en général connue, fr par défaut -->
						<attDef ident="name" usage="req" mode="change" rend="fr"/>
					</attList>
				</elementSpec>

				<elementSpec ident="district" module="namesdates">
					<desc xml:lang="fr">Région</desc>
					<desc xml:lang="en">District</desc>
					<attList>
						<attDef ident="name" usage="rec" mode="change"/>
					</attList>
				</elementSpec>

				<elementSpec ident="settlement" module="namesdates">
					<desc xml:lang="fr">Ville</desc>
					<desc xml:lang="en">Town</desc>
					<attList>
						<!-- ref= norme TGN ? Indiquer la ville, village de recueil : référentiel Getty Thesaurus of Geographic Name / code UNECE des Nations Unies plus fin que le code postal-->
						<attDef ident="ref" usage="req" mode="change"/>
					</attList>
				</elementSpec>

				<elementSpec ident="setting" module="corpus">
					<desc xml:lang="fr">Session enregistrée : description, date, environnement, langue</desc>
					<desc xml:lang="en">Setting : description, date, setting, language</desc>
					<content>
						<elementRef key="date" minOccurs="0" maxOccurs="unbounded"/>
						<!-- description de la situation ==> attention aux redondances ==> description déjà dans title -->
						<elementRef key="activity" minOccurs="0" maxOccurs="unbounded"/>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="activity" module="namesdates">
					<desc xml:lang="fr">Description de la session enregistrée</desc>
					<desc xml:lang="en">Setting description</desc>
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="p" module="core">
					<desc xml:lang="fr">Description courte</desc>
					<desc xml:lang="en">Short description</desc>
					<content>
						<textNode/>
					</content>
				</elementSpec>

				<elementSpec ident="date" module="core">
					<desc xml:lang="fr">Intervalle (depuis/jusqu'à) ou date exacte
					</desc>
					<!-- ds le modèle normalement contrainte soit when soit before/after
					-->
					<content>
						<textNode/>
					</content>
					<attList>
						<attDef ident="notBefore-iso" usage="opt" mode="change">
							<desc xml:lang="fr">Depuis</desc>
							<desc xml:lang="en">Not before</desc>
							<datatype>
								<dataRef name="date"/>
							</datatype>
						</attDef>
						<attDef ident="notAfter-iso" usage="opt" mode="change">
							<desc xml:lang="fr">Jusqu'à</desc>
							<desc xml:lang="en">Not after</desc>
							<datatype>
								<dataRef name="date"/>
							</datatype>
						</attDef>
						<attDef ident="when-iso" usage="opt" mode="change">
							<desc xml:lang="fr">Date exacte</desc>
							<desc xml:lang="en">Exact date</desc>
							<datatype>
								<dataRef name="date"/>
							</datatype>
						</attDef>
					</attList>
				</elementSpec>

				<!-- langUsage : langue de l'enregistrement, plusieurs possibles avec % d'utilisation-->
				<elementSpec ident="langUsage" module="header" mode="change" n="1">
					<desc xml:lang="fr">Langue(s) parlée dans la session (si plusieurs pourcentage d'utilisation)</desc>
					<desc xml:lang="en">Recording language(s), a percentage if several languages</desc>
					<content>
						<elementRef key="language" minOccurs="1" maxOccurs="unbounded"/>
					</content>
				</elementSpec>

				<elementSpec ident="language" module="header">
					<desc xml:lang="fr">Langue(s) de la situation, si plusieurs pourcentage d'utilisation</desc>
					<desc xml:lang="en">Situation language(s), a percentage if several languages</desc>
					<attList>
						<!-- code iso RFC 3066"-->
						<attDef ident="usage" usage="req" mode="change" rend="100"/>
						<attDef ident="ident" usage="req" mode="change" rend="fr"/>
					</attList>
				</elementSpec>

				<elementSpec ident="appInfo" module="header" mode="change" n="1">
					<desc xml:lang="fr"> Convertisseur TEI : TEI_CORPO ou un autre convertisseur</desc>
					<desc xml:lang="en">TEI conversion : TEI_CORPO or another tool</desc>
					<content>
						<sequence minOccurs="0" maxOccurs="unbounded">
							<elementRef key="application"/>
							<!-- desc:  complément sur les options/choix/logiciels "maison"-->
							<elementRef key="desc"/>
						</sequence>
					</content>
				</elementSpec>

